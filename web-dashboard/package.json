{
  "name": "poc-bdd-web-dashboard",
  "version": "1.0.0",
  "description": "Modern web dashboard for POC BDD microservices monitoring",
  "main": "server.js",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix --audit-level=high"
  },
  "dependencies": {
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "axios": "^1.7.9",
    "socket.io": "^4.8.1",
    "compression": "^1.7.5",
    "helmet": "^8.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "keywords": [
    "dashboard",
    "microservices",
    "monitoring",
    "bdd",
    "poc"
  ],
  "author": "POC BDD Team",
  "license": "MIT"
}
